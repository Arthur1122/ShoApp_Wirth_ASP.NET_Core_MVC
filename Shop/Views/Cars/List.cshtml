@using Shop.Models
@model IEnumerable<Car>


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <h2>All Cars</h2>
    @{ 
        foreach (var car in Model)
        {
            <h2>Model : @car.Name</h2>
            <p>Price : @car.Price.ToString("c")</p>
        }
    }
</body>
</html>
